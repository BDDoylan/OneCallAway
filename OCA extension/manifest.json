{
    "manifest_version": 2,
    "name": "One Call Away",
    "description": "An extension that puts you one click away to call",
    "version": "1.0.0",
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "permissions": ["activeTab"]
}